{
    "name": "@pcrab/pblive-lib",
    "version": "0.0.1",
    "description": "A simple bilibili live danmu lib",
    "license": "MIT",
    "type": "module",
    "keywords": [],
    "author": {
        "name": "Pcrab",
        "email": "pcrab@pcrab.xyz",
        "url": "https://github.com/Pcrab"
    },
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "typesVersions": {
        "*": {
            "*": [
                "./dist",
                "./dist/index.d.ts"
            ]
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "prepare": "husky install",
        "build": "unbuild",
        "check": "tsc --noEmit",
        "dev": "unbuild --stub",
        "format": "eslint src --fix"
    },
    "devDependencies": {
        "@types/jest": "^29.0.3",
        "@types/node": "^18.7.18",
        "@typescript-eslint/eslint-plugin": "^5.37.0",
        "@typescript-eslint/parser": "^5.37.0",
        "eslint": "^8.23.1",
        "husky": "^8.0.1",
        "jest": "^29.0.3",
        "ts-jest": "^29.0.1",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.3",
        "unbuild": "^0.8.11"
    },
    "dependencies": {
        "bilibili-live-ws": "^6.2.1",
        "buffer": "^6.0.3",
        "events": "^3.3.0",
        "pako": "^2.0.4"
    }
}